    <script>
        // Ajusta lightbox e legendas
        document.addEventListener('DOMContentLoaded', function () {
            var cards = document.querySelectorAll('.portfolio-item');
            cards.forEach(function (card) {
                var link = card.querySelector('.portfolio-text .d-flex a');

                if (link) {
                    // Garante que abre no lightbox
                    link.setAttribute('data-lightbox', 'portfolio');
                    link.removeAttribute('target');
                    link.removeAttribute('rel');
                }

                var titleEl = card.querySelector('.portfolio-text h4');
                if (!titleEl) return;
                var caption = document.createElement('div');
                caption.className = 'map-caption text-center';
                caption.textContent = titleEl.textContent.trim();
                card.appendChild(caption);
            });
        });
    </script>
